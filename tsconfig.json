{
    "compilerOptions": {
        "lib": [
          "ES2021"
        ],
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "sourceMap": false,
        "removeComments": true,
        "moduleResolution": "node",
        "strict": false,
        "module": "esnext",
        "target": "ES6",
        "isolatedModules": true,
    },
    "exclude": [
      "dist"
    ]
}
